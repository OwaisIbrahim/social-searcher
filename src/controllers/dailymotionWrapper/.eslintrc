{
    "rules": {
        "curly": [1, "multi-line"],
        "comma-dangle": [1, "never"],
        "complexity": [1, 10],
        "dot-notation": [1, {"allowKeywords": true}],
        "no-underscore-dangle": [0],
        "no-extra-strict": [0],
        "eqeqeq": [2, "smart"],
        "guard-for-in": [2],
        "no-else-return": [1],
        "no-eval": [2],
        "no-redeclare": [2],
        "no-return-assign": [2],
        "no-void": [1],
        "no-multi-spaces": [1, {
            "exceptions": {
                    "VariableDeclarator": true
                }
            }
        ],
        "key-spacing": [1, { align: "value" }],
        "no-extra-boolean-cast": [0],
        "quotes": [1, "single"],
        "radix": [2],
        "strict": [2],
        "no-undefined": [2],
        "no-use-before-define": [2],
        "indent": [1, 4],
        "comma-style": [1, "last"],
        "consistent-this": [1, "self"],
        "space-infix-ops": [1],
        "eol-last": [1],
        "key-spacing": [1, {
            "align": "value",
            "beforeColon": false,
            "afterColon": true
        }],
        "new-cap": [1],
        "new-parens": [1],
        "no-array-constructor": [2],
        "no-new-object": [2],
        "camelcase": [1, { "properties": "never" }]
    },
    "env": {
        "node": true
    }
}
